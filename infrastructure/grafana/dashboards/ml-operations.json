{
  "dashboard": {
    "id": null,
    "uid": "ml-operations",
    "title": "ML Operations - Narrative Modeling App",
    "tags": ["ml", "machine-learning", "training", "prediction"],
    "timezone": "browser",
    "schemaVersion": 38,
    "version": 1,
    "refresh": "30s",
    "time": {
      "from": "now-6h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["30s", "1m", "5m", "15m", "30m", "1h"],
      "time_options": ["1h", "6h", "12h", "24h", "2d", "7d", "30d"]
    },
    "templating": {
      "list": []
    },
    "annotations": {
      "list": []
    },
    "panels": [
      {
        "id": 1,
        "gridPos": {"h": 2, "w": 24, "x": 0, "y": 0},
        "type": "row",
        "title": "Training Metrics"
      },
      {
        "id": 2,
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 2},
        "type": "graph",
        "title": "Training Duration by Model Type",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum(rate(ml_training_duration_seconds_bucket[30m])) by (le, model_type))",
            "legendFormat": "{{model_type}} (p95)",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.50, sum(rate(ml_training_duration_seconds_bucket[30m])) by (le, model_type))",
            "legendFormat": "{{model_type}} (p50)",
            "refId": "B"
          }
        ],
        "yaxes": [
          {"format": "s", "label": "Duration"},
          {"format": "short"}
        ],
        "options": {
          "legend": {"displayMode": "table", "placement": "right"},
          "tooltip": {"mode": "multi", "sort": "desc"}
        }
      },
      {
        "id": 3,
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 2},
        "type": "graph",
        "title": "Training Jobs Over Time",
        "targets": [
          {
            "expr": "sum(rate(ml_training_duration_seconds_count[30m])) by (model_type)",
            "legendFormat": "{{model_type}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "ops", "label": "Jobs/min"},
          {"format": "short"}
        ],
        "options": {
          "legend": {"displayMode": "list", "placement": "bottom"},
          "tooltip": {"mode": "multi", "sort": "desc"}
        }
      },
      {
        "id": 4,
        "gridPos": {"h": 2, "w": 24, "x": 0, "y": 10},
        "type": "row",
        "title": "Prediction Performance"
      },
      {
        "id": 5,
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 12},
        "type": "graph",
        "title": "Prediction Latency (p50, p95, p99)",
        "targets": [
          {
            "expr": "histogram_quantile(0.50, sum(rate(ml_prediction_latency_seconds_bucket[5m])) by (le))",
            "legendFormat": "p50",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, sum(rate(ml_prediction_latency_seconds_bucket[5m])) by (le))",
            "legendFormat": "p95",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.99, sum(rate(ml_prediction_latency_seconds_bucket[5m])) by (le))",
            "legendFormat": "p99",
            "refId": "C"
          }
        ],
        "yaxes": [
          {"format": "s", "label": "Latency"},
          {"format": "short"}
        ],
        "options": {
          "legend": {"displayMode": "list", "placement": "bottom"},
          "tooltip": {"mode": "multi", "sort": "none"}
        },
        "alert": {
          "name": "High Prediction Latency",
          "conditions": [
            {
              "evaluator": {"params": [1], "type": "gt"},
              "operator": {"type": "and"},
              "query": {"params": ["C", "5m", "now"]},
              "reducer": {"params": [], "type": "avg"},
              "type": "query"
            }
          ],
          "executionErrorState": "alerting",
          "frequency": "1m",
          "handler": 1,
          "message": "p99 prediction latency is above 1 second",
          "noDataState": "no_data"
        }
      },
      {
        "id": 6,
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 12},
        "type": "graph",
        "title": "Prediction Throughput by Model",
        "targets": [
          {
            "expr": "sum(rate(ml_prediction_latency_seconds_count[5m])) by (model_type)",
            "legendFormat": "{{model_type}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "ops", "label": "Predictions/sec"},
          {"format": "short"}
        ],
        "options": {
          "legend": {"displayMode": "table", "placement": "right"},
          "tooltip": {"mode": "multi", "sort": "desc"}
        }
      },
      {
        "id": 7,
        "gridPos": {"h": 2, "w": 24, "x": 0, "y": 20},
        "type": "row",
        "title": "Model Performance"
      },
      {
        "id": 8,
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 22},
        "type": "graph",
        "title": "Model Accuracy Trends",
        "targets": [
          {
            "expr": "ml_model_accuracy{metric_name=\"accuracy\"}",
            "legendFormat": "{{model_type}} - {{model_id}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "percentunit", "label": "Accuracy", "min": 0, "max": 1},
          {"format": "short"}
        ],
        "options": {
          "legend": {"displayMode": "table", "placement": "right"},
          "tooltip": {"mode": "multi", "sort": "desc"}
        }
      },
      {
        "id": 9,
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 22},
        "type": "graph",
        "title": "Dataset Size Distribution",
        "targets": [
          {
            "expr": "sum(rate(ml_dataset_size_rows_count[30m])) by (le)",
            "legendFormat": "{{le}} rows",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "short", "label": "Datasets"},
          {"format": "short"}
        ],
        "options": {
          "legend": {"displayMode": "list", "placement": "bottom"},
          "tooltip": {"mode": "multi", "sort": "none"}
        }
      },
      {
        "id": 10,
        "gridPos": {"h": 2, "w": 24, "x": 0, "y": 30},
        "type": "row",
        "title": "Summary Statistics"
      },
      {
        "id": 11,
        "gridPos": {"h": 6, "w": 6, "x": 0, "y": 32},
        "type": "stat",
        "title": "Total Training Jobs (24h)",
        "targets": [
          {
            "expr": "sum(increase(ml_training_duration_seconds_count[24h]))",
            "refId": "A"
          }
        ],
        "options": {
          "reduceOptions": {"values": false, "fields": "", "calcs": ["lastNotNull"]},
          "text": {"titleSize": 16, "valueSize": 40},
          "graphMode": "none",
          "colorMode": "value",
          "orientation": "auto"
        }
      },
      {
        "id": 12,
        "gridPos": {"h": 6, "w": 6, "x": 6, "y": 32},
        "type": "stat",
        "title": "Total Predictions (24h)",
        "targets": [
          {
            "expr": "sum(increase(ml_prediction_latency_seconds_count[24h]))",
            "refId": "A"
          }
        ],
        "options": {
          "reduceOptions": {"values": false, "fields": "", "calcs": ["lastNotNull"]},
          "text": {"titleSize": 16, "valueSize": 40},
          "graphMode": "none",
          "colorMode": "value",
          "orientation": "auto"
        }
      },
      {
        "id": 13,
        "gridPos": {"h": 6, "w": 6, "x": 12, "y": 32},
        "type": "stat",
        "title": "Avg Training Duration",
        "targets": [
          {
            "expr": "avg(rate(ml_training_duration_seconds_sum[1h]) / rate(ml_training_duration_seconds_count[1h]))",
            "refId": "A"
          }
        ],
        "options": {
          "reduceOptions": {"values": false, "fields": "", "calcs": ["lastNotNull"]},
          "text": {"titleSize": 16, "valueSize": 40},
          "graphMode": "area",
          "colorMode": "value",
          "orientation": "auto"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "s"
          }
        }
      },
      {
        "id": 14,
        "gridPos": {"h": 6, "w": 6, "x": 18, "y": 32},
        "type": "stat",
        "title": "Avg Prediction Latency",
        "targets": [
          {
            "expr": "avg(rate(ml_prediction_latency_seconds_sum[1h]) / rate(ml_prediction_latency_seconds_count[1h]))",
            "refId": "A"
          }
        ],
        "options": {
          "reduceOptions": {"values": false, "fields": "", "calcs": ["lastNotNull"]},
          "text": {"titleSize": 16, "valueSize": 40},
          "graphMode": "area",
          "colorMode": "value",
          "orientation": "auto"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": null, "color": "green"},
                {"value": 0.5, "color": "yellow"},
                {"value": 1, "color": "red"}
              ]
            }
          }
        }
      }
    ]
  }
}
